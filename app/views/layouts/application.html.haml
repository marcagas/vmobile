!!!html

%html
  %head
    %title VMOBILE
    = stylesheet_link_tag "application", :media => 'all'
    = javascript_include_tag "application"
    = csrf_meta_tags

  %body
    #head
      = render :partial => 'layouts/header'
      -#= render :partial => 'layouts/header_nav'

    #container
      .container-fluid
        .row-fluid
          #sidebar.span2
            = render :partial => 'layouts/sidebar'
          .span10
            = yield

    #footer
      -#= render :partial => 'layouts/footer'